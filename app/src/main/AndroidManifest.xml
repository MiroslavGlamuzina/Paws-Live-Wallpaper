<?xml version="1.0" encoding="utf-8"?>


<manifest package="com.authorwjf.live_wallpaper_p1"


          xmlns:android="http://schemas.android.com/apk/res/android"
          android:versionCode="1"
          android:versionName="1.0">

    <application
        android:icon="@android:drawable/alert_dark_frame"
        android:label="TR Demo Live Wallpaper">
        <service
            android:name="remingtonproductions.pawslivewallpaper.PawsLiveWallpaperService"
            android:label="TR Demo Live Wallpaper"

            android:permission="android.permission.BIND_WALLPAPER">
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService"/>
            </intent-filter>
            <meta-data
                android:name="android.service.wallpaper"
                android:resource="@xml/wallpaper"/>
        </service>
    </application>
    <uses-sdk android:minSdkVersion="7"/>
    <uses-feature android:name="android.software.live_wallpaper"/>
</manifest>